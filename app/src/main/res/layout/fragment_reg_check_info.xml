<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dialog.RegCheckInfoFragment">

    <ImageView
        android:id="@+id/reg_check_info_background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/reg_check_info_background">
    </ImageView>

    <TextView
        android:id="@+id/reg_check_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/reg_check_info_background"
        app:layout_constraintEnd_toEndOf="@+id/reg_check_info_background"
        app:layout_constraintTop_toTopOf="@+id/reg_check_info_background"
        android:textColor="@color/black"
        android:layout_marginTop="30dp"
        android:textSize="20sp"
        android:text="등록 정보를 확인하세요.">
    </TextView>

    <TextView
        android:id="@+id/reg_check_info_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="휴대폰번호가 인증되지 않았습니다."
        app:layout_constraintStart_toStartOf="@+id/reg_check_title"
        app:layout_constraintEnd_toEndOf="@+id/reg_check_title"
        app:layout_constraintTop_toBottomOf="@+id/reg_check_title"
        android:layout_marginTop="50dp">
    </TextView>
    <ImageView
        android:id="@+id/info_close_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/reg_check_info_background"
        app:layout_constraintBottom_toBottomOf="@+id/reg_check_info_background"
        android:layout_marginBottom="30dp"
        android:layout_marginLeft="55dp"
        android:src="@drawable/info_close">
    </ImageView>
    <ImageView
        android:id="@+id/check3_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="@+id/reg_check_info_background"
        app:layout_constraintBottom_toBottomOf="@+id/reg_check_info_background"
        android:layout_marginBottom="30dp"
        android:layout_marginRight="55dp"
        android:src="@drawable/check3">

    </ImageView>


</androidx.constraintlayout.widget.ConstraintLayout>