<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".dialog.DialogwithdrawalFragment">

    <ImageView
        android:id="@+id/dialog_withdrawal_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/dialog_withdrawal">
    </ImageView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/dialog_withdrawal_back"
        app:layout_constraintTop_toTopOf="@+id/dialog_withdrawal_back"
        app:layout_constraintEnd_toEndOf="@+id/dialog_withdrawal_back"
        android:layout_marginTop="30dp"
        android:text="회원 탈퇴 완료"
        android:textSize="23sp"
        android:textColor="@color/black"
        android:textStyle="bold">
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/dialog_withdrawal_back"
        app:layout_constraintTop_toTopOf="@+id/dialog_withdrawal_back"
        app:layout_constraintEnd_toEndOf="@+id/dialog_withdrawal_back"
        android:layout_marginTop="120dp"
        android:text="회원 탈퇴가 완료되었습니다.">
    </TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/dialog_withdrawal_back"
        app:layout_constraintTop_toTopOf="@+id/dialog_withdrawal_back"
        app:layout_constraintEnd_toEndOf="@+id/dialog_withdrawal_back"
        android:layout_marginTop="150dp"
        android:text="메인 화면으로 이동합니다.">
    </TextView>

    <ImageView
        android:id="@+id/withdrawal_ckeckcomp_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@+id/dialog_withdrawal_back"
        app:layout_constraintBottom_toBottomOf="@+id/dialog_withdrawal_back"
        app:layout_constraintEnd_toEndOf="@+id/dialog_withdrawal_back"
        android:layout_marginBottom="30dp"
        android:src="@drawable/withdrawal_check_btn">
    </ImageView>

</androidx.constraintlayout.widget.ConstraintLayout>